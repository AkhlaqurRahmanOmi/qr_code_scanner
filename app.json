{
  "expo": {
    "name": "ExpoQR2",
    "slug": "ExpoQR2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/scanner.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/AkhlaqurRahmanOmi.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wa2goose.ExpoQR2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.wa2goose.ExpoQR2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/scanner.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c4512320-10eb-4f78-90ce-5c244bc45646"
      }
    }
  }
}
